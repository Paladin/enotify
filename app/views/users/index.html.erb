<h2>Users</h2>
<%= will_paginate @users %>
<br/>
<table>
  <tr>
	<th>Login</th>
	<th>Email</th>
	<th>Created At</th>
  </tr>

<% for user in @users %>
  <tr>
    <td><%=h user.login %></td>
  	<td><%=mail_to(user.email) %></td>
  	<td><%=h user.created_at.to_formatted_s(:simple) %></td>
  </tr>
<% end %>
</table>
<br/>
<%= will_paginate @users %>