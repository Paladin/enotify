<h1>Listing service_requests</h1>

<table>
  <tr>
	<th>Record Number</th>
	<th>Tax Key</th>
	<th>Time</th>
	<th>Description</th>
	<th>Address</th>
	<th>Geo Location</th>
  </tr>

<% for service_request in @service_requests %>
  <tr>
  	<td><%=h service_request.record_number %></td>
  	<td><%=h service_request.tax_key %></td>
  	<td><%=h service_request.time.to_formatted_s(:short) %></td>
  	<td><%=h service_request.description %></td>
 	<td><%=h service_request.address.to_s %></td>
	<td><%=h service_request.geo_location.to_s %></td>
	<td><%= link_to 'Show', service_request %></td>
	<td><%= link_to 'Edit', edit_service_request_path(service_request) %></td>
	<td><%= link_to 'Destroy', service_request, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New service_request', new_service_request_path %>
